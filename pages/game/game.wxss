/* pages/list/list.wxss */
.swiper {
  height: 400rpx;
}
.swiper image {
  height: 100%;
  width: 100%;
}

.scroll-view {
  width: 100%;
  height: 100vh;
}
.icon-image {
  width:20px;
  height:20px;
  margin-right: 10px;
  padding-top:5px;
}

.searchbar-result{
    margin-top: 0;
    font-size: 14px;
}

.searchbar-fixed {
    position: absolute;
    top:0px;
}

.searchbar-result:before{
    display: none;
}
.weui-cell{
    padding: 12px 15px 12px 35px;
}