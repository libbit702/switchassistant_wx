/* 表格代码   */
.table{
  border-right:0;
  border-bottom: 0;
  width: 98%;
  margin-left: 1%;
}
.tr{
  width:100%;
  display: flex;
  justify-content: space-between;
  border-bottom: 1px solid #f0f0f0;
}
.th,.td{
  padding: 10px;
  width: 100%;
}
.th1,.th2,.td1,.td2{
  width: 40%;
}
.price-position {
  width:30px;
}
.price-country {
  width:100%;
}
.price-meta {
  width:280px;
}
.price-value {
  width:110px;
}
.th{
  font-weight: 800;
  font-size: 28rpx;
  color: #330e0e;
  text-align:left;
}
.td{
  font-size: 0.8rem;
  color: #ec9480;
  display: flex;
  align-items:right; 
  text-align: center;
}

.price-country image {
  width:20px;
  height:20px;
  margin-right: 10px;
}

.title {
  font-weight:500;
  font-size:1.4em;
}

.desc,.release {
  font-size:0.8em;
  font-weight:80;
  -webkit-font-smoothing: antialiased;
}

.price-position image {
  width:30px;
  height:30px;
  /* margin:0 0 10px; */
}

.price-country image {
  margin-right:5px;
}
